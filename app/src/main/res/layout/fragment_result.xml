<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".ResultFragment">


    <Button
        android:id="@+id/requestAccessbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="access 발급 요청" />

    <TextView
        android:id="@+id/authorizationcode_content"
        android:layout_width="263dp"
        android:layout_height="337dp"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="93dp"
        android:layout_toRightOf="@+id/authorizationcode"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/authorizationcode"
        android:layout_width="130dp"
        android:layout_height="51dp"
        android:layout_below="@+id/requestAccessbtn"
        android:text="authorization_code"
        android:textColor="#FFFFFF" />


</RelativeLayout>
